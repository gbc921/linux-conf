# /etc/config/firewall
config redirect
	option target 'DNAT'
	option src 'wan'
	option dest 'lan'
	option proto 'tcp udp'
	option dest_ip '192.168.1.10'
	option name 'AmericasArmy'
	option src_dport '7778-7780'
	option dest_port '7778-7780'
	option enabled '0'

